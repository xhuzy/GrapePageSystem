<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>SelectFundInfo</servlet-name>
  	<display-name>SelectFundInfo</display-name>
  	<description>User This Servlet to show fundinfo</description>
  	<servlet-class>com.youyou.grapepage.servlet.SelectFundInfo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DefaultServlet</servlet-name>
  	<display-name>DefaultServlet</display-name>
  	<description></description>
  	<servlet-class>com.youyou.grapepage.servlet.DefaultServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SelectFundInfo</servlet-name>
  	<url-pattern>/SelectFundInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DefaultServlet</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  
  
</web-app>
